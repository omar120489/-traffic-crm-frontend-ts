{
  "name": "@traffic-crm/sdk-js",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "dependencies": {
    "ky": "^1.5.0"
  },
  "devDependencies": {
    "openapi-typescript": "^7.4.2",
    "typescript": "^5.6.3"
  },
  "scripts": {
    "gen": "openapi-typescript http://localhost:3000/openapi.json -o src/types.gen.ts",
    "codegen": "openapi-typescript ../../apps/core-api/openapi.json -o src/types.gen.ts",
    "typecheck": "tsc --noEmit",
    "build": "tsc -p tsconfig.json",
    "prepublishOnly": "pnpm build"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
